{
  "version": 2,
  "builds": [
    { "src": "next.config.mjs", "use": "@vercel/next" },
    { "src": "src/dist/**", "use": "@vercel/static" },
    { "src": "twig/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/vue/(.*)", "dest": "/src/dist/$1" },
    { "src": "/twig/(.*)", "dest": "/twig/$1" },
    { "src": "/(.*)", "dest": "/app/$1" }
  ]
}
